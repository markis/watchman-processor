{
  "name": "watchman-processor",
  "version": "2.0.4",
  "description": "Processor for the fb-watchman service",
  "keywords": [
    "watchman",
    "rsync",
    "synchronization",
    "file-synchronization",
    "file-watcher"
  ],
  "author": "Markis Taylor <m@rkis.cc>",
  "main": "index.js",
  "bin": "./bin/watchman-processor",
  "typings": "index.d.ts",
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "fb-watchman": "^2.0.0",
    "inversify": "^3.1.0",
    "node-emoji": "^1.5.1",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/chalk": "^0.4.31",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "@types/reflect-metadata": "0.0.5",
    "@types/sinon": "^2.1.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "pre-commit": "^1.2.2",
    "rollup": "^0.41.5",
    "rollup-plugin-typescript": "^0.8.1",
    "sinon": "^2.0.0",
    "ts-helpers": "^1.1.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "scripts": {
    "compile": "tsc --noEmitHelpers --module amd --out index.js && tsc --noEmitHelpers && rollup -c",
    "coverage": "npm run compile && istanbul cover _mocha built/test/**/*-test.js",
    "lint": "tslint -c tslint.json src/*.ts test/*.ts",
    "test": "npm run lint && npm run coverage"
  },
  "pre-commit": [
    "lint",
    "coverage"
  ],
  "repository": "https://github.com/markis/watchman-processor.git",
  "license": "MIT"
}
